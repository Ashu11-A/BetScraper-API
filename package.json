{
  "name": "node",
  "version": "1.0.0",
  "main": "dist/app.js",
  "type": "module",
  "private": "true",
  "scripts": {
    "dev": "bun --watch src/app.ts",
    "build": "rimraf dist && tsup && tsc-alias -p ./tsconfig.json",
    "lint": "eslint -c eslint.config.mjs",
    "typeorm": "typeorm-ts-node-esm -d ./src/database/dataSource.ts",
    "schema:sync": "bun run typeorm -- schema:sync",
    "typeorm:cache": "bun run typeorm -- cache:clear",
    "schema:drop": "bun run typeorm -- schema:drop",
    "migration:generate": "bun run typeorm -- migration:generate ./src/database/migration/schema-update",
    "migration:show": "bun run typeorm -- migration:show",
    "migration:run": "bun run typeorm -- migration:run",
    "migration:revert": "bun run typeorm -- migration:revert"
  },
  "tsup": {
    "platform": "node",
    "format": "esm",
    "entry": ["src/**/*.ts"],
    "outDir": "dist/mjs/src",
    "tsconfig": "./tsconfig.json",
    "bundle": false,
    "minify": true,
    "minifyIdentifiers": true,
    "minifySyntax": true,
    "minifyWhitespace": true,
    "skipNodeModulesBundle": true,
    "splitting": true,
    "clean": true,
    "dts": false
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/bcrypt": "^5.0.2",
    "@types/bun": "latest",
    "@types/glob": "^8.1.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.2",
    "@types/sharp": "^0.32.0",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.1",
    "wrap-ansi": "7.0.0",
    "string-width": "4.1.0"
  },
  "dependencies": {
    "@fastify/cookie": "9.4.0",
    "@fastify/formbody": "^7.4.0",
    "@fastify/multipart": "^8.3.0",
    "@fastify/passport": "2.5.0",
    "@fastify/session": "10.9.0",
    "@fastify/static": "7.0.4",
    "@fastify/websocket": "^10.0.1",
    "@techstark/opencv-js": "^4.10.0-release.1",
    "@types/cli-progress": "^3.11.6",
    "@types/string-similarity": "^4.0.2",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "bull": "4.16.3",
    "chalk": "^5.4.0",
    "cli-progress": "^3.12.0",
    "cron-expression-validator": "^1.0.20",
    "csv": "^6.3.11",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "exceljs": "^4.4.0",
    "fastify": "4.28.1",
    "glob": "11.0.0",
    "image-hash": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^11.0.2",
    "mysql": "^2.18.1",
    "nanoid": "^5.0.9",
    "p-limit": "^6.1.0",
    "pino-pretty": "^11.3.0",
    "puppeteer": "^23.11.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.33.5",
    "string-comparison": "^1.3.0",
    "string-similarity": "^4.0.4",
    "tesseract.js": "^5.1.1",
    "typeorm": "^0.3.20",
    "zod": "^3.24.1"
  }
}